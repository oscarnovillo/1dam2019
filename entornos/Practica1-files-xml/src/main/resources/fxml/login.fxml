            xmlns:fx="http://javafx.com/fxml/1" fx:controller="fx.controllers.LoginController">
    <children>
        <Label layoutX="159.0" layoutY="129.0" text="User:"/>
        <Label layoutX="139.0" layoutY="168.0" text="Password:"/>
        <TextField fx:id="fxUsuario" layoutX="246.0" layoutY="125.0"/>
