<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="470.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fx.controllers.PantallaAddStudentsToProfesorsController">
    <children>
        <Label layoutX="30.0" layoutY="29.0" text="Teachers" />
        <ListView fx:id="fxAllStudents" layoutX="24.0" layoutY="135.0" onMouseClicked="#allStudentSelected" prefHeight="200.0" prefWidth="200.0" />
        <ListView fx:id="fxStudentsAssigned" layoutX="369.0" layoutY="135.0" onMouseClicked="#studentSelected" prefHeight="200.0" prefWidth="200.0" />
        <Button fx:id="fxBtAssingStudent" layoutX="240.0" layoutY="199.0" mnemonicParsing="false" onAction="#assignedStudent" text="Asigned Student -&gt;" />
        <Button fx:id="fxBtUnAssingStudent" layoutX="231.0" layoutY="277.0" mnemonicParsing="false" onAction="#unAssignedStudent" text="&lt;- Unassigned student" />
      <ComboBox fx:id="fxTeachers" layoutX="124.0" layoutY="34.0" onAction="#selectTeacher" prefHeight="25.0" prefWidth="409.0" />
    </children>
</AnchorPane>
